---
layout: default
---

<% for section in @sections : %>
  <!-- <%= section.toUpperCase() %> -->

  <section class="section <%= section %>" id="<%= section %>" data-section>
    <% if (index % 2) : %>
      <div class="wrapper">
        <%- @partial("html/section/#{section}.html.eco", @) %>
      </div>
    <% else : %>
      <%- @partial("html/section/#{section}.html.eco", @) %>
    <% end %>
  </section>

  <!-- / <%= section.toUpperCase() %> -->
<% end %>
